<div id="subheader">
	<div class="inner">
		<div class="container">
			<h1>Your Documents</h1>
		</div> <!-- /.container -->
	</div> <!-- /inner -->
</div> <!-- /subheader -->


<% @documents.each do |document| %>
  <tr>
	<td><a href="/documents/<%=document.id%>"><img src="https://crocodoc.com/api/v2/download/thumbnail?token=<%= @api_token %>&uuid=<%= document.uuid %>&size=300x300"></a></td>
    <td><%= link_to 'Edit', edit_document_path(document) %></td>
    <td><%= link_to 'Destroy', document, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Document', new_document_path %>

